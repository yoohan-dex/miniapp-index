.html{position:absolute;width:100%;height:56%;background:radial-gradient(ellipse at bottom, #1b2735 0, #090a0f 100%);overflow:hidden}@keyframes spin{from{transform:rotate(0deg)}to{transform:rotate(360deg)}}.body{background:radial-gradient(ellipse at bottom, #1B2735 0, #090A0F 100%);height:100%}.space{position:absolute;width:400vw;height:400vh;top:50%;left:50%;margin-top:-200vh;margin-left:-200vw;animation:spin 240s linear infinite;background-size:140px;backface-visibility:visible;background-image:url(data:image/svg+xml;base64,)}.title{position:absolute;top:50%;left:0;right:0;color:#FFF;text-align:center;font-weight:300;letter-spacing:10rpx;margin-top:-60rpx;padding-left:10rpx}.title text{background:-webkit-linear-gradient(white, #38495a);-webkit-background-clip:text;-webkit-text-fill-color:transparent}.title .sup{font-size:70rpx}.title .sub{font-size:50rpx}